<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:element name="Obj" type="XObj"/>

    <xs:element name="Data" type="Data"/>
    
    <xs:element name="RL" type="XRL"/>
    
    <xs:element name="XML" type="Header"/>

    <xs:complexType name="Header">
        <xs:sequence>
            <xs:element name="Data" type="XData" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Objs" type="Objs" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="iDb_Revision" type="xs:int" fixed="4204"/>
        <xs:attribute name="fDate" type="xs:double"/>
        <xs:attribute name="ID_OT" type="xs:int"/>
        <xs:attribute name="sOT" type="xs:string"/>
        <xs:attribute name="ID_User" type="xs:int"/>
        <xs:attribute name="sUser" type="xs:string"/>


    </xs:complexType>

    <!-- Base complex Types -->

    <xs:complexType name="XID">
        <xs:annotation>
            <xs:documentation xml:lang="en"> Base object. ID is an identifier into a given table . TD (for data type) identifies the table the object belongs to. </xs:documentation>
        </xs:annotation>
        <xs:attribute name="ID" type="xs:int" use="optional"/>
        <xs:attribute name="TD" type="xs:int" use="optional"/>
        <xs:attribute name="iDB" type="xs:int" use="optional"/>
        <xs:attribute name="iDB_LM" type="xs:int" use="optional"/>
    </xs:complexType>

    <xs:complexType name="XNamed">
        <xs:annotation>
            <xs:documentation xml:lang="en">Named and ordered object.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XID">
                <xs:attribute name="sName" type="xs:string" use="optional"/>
                <xs:attribute name="iOrder" type="xs:int" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XTree">
        <xs:annotation>
            <xs:documentation xml:lang="en">Tree organized object. </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XNamed">
                <xs:attribute name="ID_Parent" type="xs:int" use="optional"/>
                <xs:attribute name="iIcon" type="xs:int" use="optional"/>
                <xs:attribute name="bParent" type="xs:boolean" use="optional"/>
                <xs:attribute name="bFolder" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!-- TEEXMA complex Types -->

    <xs:complexType name="XObj">
        <xs:annotation>
            <xs:documentation xml:lang="en">Object.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XTree">
                <xs:sequence>
                    <xs:element name="Data" type="Data" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="Srcs" type="Srcs" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
                <xs:attribute name="ID_OT" type="xs:int" use="optional"/>
                <xs:attribute name="ID_Obj_Owner" type="xs:int" use="optional"/>
                <xs:attribute name="fDate" type="xs:double" use="optional"/>
                <xs:attribute name="sOT" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XData">
        <xs:annotation>
            <xs:documentation xml:lang="en">Base data class (virtual).</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XID">
                <xs:attribute name="ID_Obj" type="xs:int" use="optional"/>
                <xs:attribute name="ID_Att" type="xs:int" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XDBool">
        <xs:annotation>
            <xs:documentation xml:lang="en">Boolean data.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XData">
                <xs:attribute name="bVal" type="xs:boolean" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XDStr">
        <xs:annotation>
            <xs:documentation xml:lang="en">String data (also handles text and email data).</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XData">
                <xs:attribute name="sVal" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XDDec">
        <xs:annotation>
            <xs:documentation xml:lang="en">Decimal data (min/max/mean data).</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XData">
                <xs:attribute name="ID_Unit" type="xs:int" use="optional"/>
                <xs:attribute name="fMin" type="xs:float" use="required"/>
                <xs:attribute name="fMax" type="xs:float" use="optional"/>
                <xs:attribute name="fMean" type="xs:float" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XDDate">
        <xs:annotation>
            <xs:documentation xml:lang="en">Date and datetime data.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XData">
                <xs:attribute name="fVal" type="xs:double" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XAF">
        <xs:annotation>
            <xs:documentation xml:lang="en">Archived File .</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XNamed">
                <xs:attribute name="iIndex_Global" type="xs:int" use="optional"/>
                <xs:attribute name="iIndex" type="xs:int" use="optional"/>
                <xs:attribute name="iVersion" type="xs:int" use="optional"/>
                <xs:attribute name="sDate" type="xs:string" use="optional"/>
                <xs:attribute name="sLeft_Ext" type="xs:string" use="optional"/>
                <xs:attribute name="sPath_File" type="xs:string" use="optional"/>
                <xs:attribute name="sRPath_File" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XDFile">
        <xs:annotation>
            <xs:documentation xml:lang="en">Document data.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XData">
                <xs:sequence>
                    <xs:element name="AF" type="XAF" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
                <xs:attribute name="sName" type="xs:string" use="required"/>
                <xs:attribute name="bView" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XDURL">
        <xs:annotation>
            <xs:documentation xml:lang="en">URL data.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XDStr">
                <xs:attribute name="bView" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XSerie">
        <xs:annotation>
            <xs:documentation xml:lang="en">Series (belongs to a table data). Table values are described as a string of values separated by the value separator. ID_ST is the identifier of the Series Type the Series belong to. </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XNamed">
                <xs:attribute name="ID_ST" type="xs:int" use="optional"/>
                <xs:attribute name="sST" type="xs:string" use="optional"/>
                <xs:attribute name="sVal" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XDTble">
        <xs:annotation>
            <xs:documentation xml:lang="en">Table data (a list of series).</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XData">
                <xs:sequence>
                    <xs:element name="Series" type="Series" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="Graphs" type="Graphs" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XDLnk">
        <xs:annotation>
            <xs:documentation xml:lang="en">Link data.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XData">
                <xs:sequence>
                    <xs:element name="Obj" type="XObj" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="sVal" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XRL">
        <xs:annotation>
            <xs:documentation xml:lang="en">Requirement list.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XNamed">
                <xs:choice minOccurs="1" maxOccurs="2">
                    <xs:element name="PreselectionCriterion" type="XCriterion" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="SubRLs" minOccurs="0" maxOccurs="1"/>
                </xs:choice>
                <xs:attribute name="ID_OT" type="xs:int"/>
                <xs:attribute name="sAggregation" type="xs:string" use="required"/>
                <xs:attribute name="fDate" type="xs:double" use="optional"/>
                <xs:attribute name="fWeight" type="xs:float" use="optional"/>
                <xs:attribute name="sRLType" type="xs:string" use="required"/>
                <xs:attribute name="idChoiceGuide" type="xs:int" use="optional"/>
                <xs:attribute name="idQuestion" type="xs:int" use="optional"/>
                <xs:attribute name="idOwnerRL" type="xs:int" use="optional"/>
                <xs:attribute name="idExportation" type="xs:int" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XCriterion">
        <xs:annotation>
            <xs:documentation xml:lang="en">Criterion.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XNamed">
                <xs:choice minOccurs="0" maxOccurs="1">
                    <xs:element name="PreselectionCriterion" type="XCriterion" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="SubCriteria" minOccurs="0" maxOccurs="1">
                        <xs:complexType>
                            <xs:sequence minOccurs="0" maxOccurs="unbounded">
                                <xs:element name="Criterion" type="XCriterion" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="TableCriterion" type="XTableCriterion" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="NumericalCriterion" type="XNumericalCriterion" minOccurs="0" maxOccurs="1"/>
                </xs:choice>
                <xs:attribute name="idRL" type="xs:int" use="optional"/>
                <xs:attribute name="ID_OT" type="xs:int" use="required"/>
                <xs:attribute name="ID_Att" type="xs:int" use="optional"/>
                <xs:attribute name="bSelection" type="xs:boolean" use="optional"/>
                <xs:attribute name="sCritType" type="xs:string" use="required"/>
                <xs:attribute name="sEmptyDataTreatment" type="xs:string" use="optional"/>
                <xs:attribute name="sDataTreatment" type="xs:string" use="optional"/>
                <xs:attribute name="sQuestionsIds" type="xs:string" use="optional"/>
                <xs:attribute name="fWeight" type="xs:float" use="optional"/>
                <xs:attribute name="fCreationDate" type="xs:float" use="optional"/>
                <xs:attribute name="bVisible" type="xs:boolean" use="optional"/>
                <xs:attribute name="bForceCriterion" type="xs:boolean" use="optional"/>
                
                <xs:attribute name="sBooleanCritType" type="xs:string" use="optional"/>
                
                <xs:attribute name="sOptimizationCritType" type="xs:string" use="optional"/>
                <xs:attribute name="fOptimizationValue" type="xs:float" use="optional"/>
                <xs:attribute name="idOptimizationUnit" type="xs:int" use="optional"/>
                
                <xs:attribute name="sSearchedValue" type="xs:string" use="optional"/>

                <xs:attribute name="sLinkCritType" type="xs:string" use="optional"/>
                <xs:attribute name="sRecursivityType" type="xs:string" use="optional"/>
                <xs:attribute name="sPreselectionType" type="xs:string" use="optional"/>
                <xs:attribute name="sPreselection" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="XNumericalCriterion">
        <xs:attribute name="sNumericalCritType" type="xs:string" use="required"/>
        <xs:attribute name="sLBCritType" type="xs:string" use="optional"/>
        <xs:attribute name="fLBValue" type="xs:float" use="optional"/>
        <xs:attribute name="fLBFzness" type="xs:float" use="optional"/>
        <xs:attribute name="idLBUnit" type="xs:int" use="optional"/>
        <xs:attribute name="sUBCritType" type="xs:string" use="optional"/>
        <xs:attribute name="fUBValue" type="xs:float" use="optional"/>
        <xs:attribute name="fUBFzness" type="xs:float" use="optional"/>
        <xs:attribute name="idUBUnit" type="xs:int" use="optional"/>
    </xs:complexType>
    
    <xs:complexType name="XTableSubCriterion">
        <xs:complexContent>
            <xs:extension base="XNumericalCriterion">
                <xs:attribute name="sSubCriterionType" type="xs:string" use="required"/>
                <xs:attribute name="sSearchedValue" type="xs:string" use="optional"/>
                <xs:attribute name="idSeriesType" type="xs:int" use="required"/>
                <xs:attribute name="iValueIndex" type="xs:int" use="required"/>
                
            </xs:extension>
        </xs:complexContent>      
    </xs:complexType>
    
    <xs:complexType name="XTableCriterion">
        <xs:choice minOccurs="1" maxOccurs="2">
            <xs:element name="TableSubCriterion1" minOccurs="0" maxOccurs="1" type="XTableSubCriterion"/>
            <xs:element name="TableSubCriterion2" minOccurs="0" maxOccurs="1" type="XTableSubCriterion"/>
            <xs:element name="TableInterpolationCriterion" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="XNumericalCriterion">
                            <xs:attribute name="idXSeriesType" type="xs:int" use="required"/>
                            <xs:attribute name="idYSeriesType" type="xs:int" use="required"/>
                            <xs:attribute name="fXValue" type="xs:float" use="required"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
        </xs:choice>
        <xs:attribute name="sTableCritType" type="xs:string" use="required"/>
    </xs:complexType>

    <xs:complexType name="XOT">
        <xs:annotation>
            <xs:documentation xml:lang="en">Object Type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XTree">
                <xs:sequence>
                    <xs:element name="Att" type="XAtt" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="iType" type="xs:int" use="required"/>
                <xs:attribute name="bDistinct_Names" type="xs:boolean" use="optional"/>
                <xs:attribute name="bVisible" type="xs:boolean" use="optional"/>
                <xs:attribute name="iTag" type="xs:int" use="optional"/>
                <xs:attribute name="sDescription" type="xs:string" use="optional"/>
                <xs:attribute name="sProcesses" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XAtt">
        <xs:annotation>
            <xs:documentation xml:lang="en">Attribute.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XTree">
                <xs:sequence>
                    <xs:element name="Units" type="Units" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="Att" type="XAtt" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="TD_Data" type="xs:int" use="required"/>
                <xs:attribute name="ID_Att_Inheritage" type="xs:int" use="optional"/>
                <xs:attribute name="bTracking" type="xs:boolean" use="optional"/>
                <xs:attribute name="bDynamic" type="xs:boolean" use="optional"/>
                <xs:attribute name="ID_Obj_Info" type="xs:int" use="optional"/>
                <xs:attribute name="ID_LT" type="xs:int" use="optional"/>
                <xs:attribute name="ID_Unit" type="xs:int" use="optional"/>
                <xs:attribute name="ID_FT" type="xs:int" use="optional"/>
                <xs:attribute name="bList" type="xs:boolean" use="optional"/>
                <xs:attribute name="iFF" type="xs:int" use="optional"/>
                <xs:attribute name="iPrec" type="xs:int" use="optional"/>
                <xs:attribute name="iDgts" type="xs:int" use="optional"/>
                <xs:attribute name="fLB" type="xs:float" use="optional"/>
                <xs:attribute name="bLB_Inc" type="xs:boolean" use="optional"/>
                <xs:attribute name="fUB" type="xs:float" use="optional"/>
                <xs:attribute name="bUB_Inc" type="xs:boolean" use="optional"/>
                <xs:attribute name="bDisp_In_MU" type="xs:boolean" use="optional"/>
                <xs:attribute name="bInherited" type="xs:boolean" use="optional"/>
                <xs:attribute name="iTag" type="xs:int" use="optional"/>
                <xs:attribute name="ID_TT" type="xs:int" use="optional"/>
                <xs:attribute name="ID_OT" type="xs:int" use="optional"/>
                <xs:attribute name="bDisp_Table" type="xs:boolean" use="optional"/>
                <xs:attribute name="bTranspose" type="xs:boolean" use="optional"/>
                <xs:attribute name="bDisp_SN" type="xs:boolean" use="optional"/>
                <xs:attribute name="bDisp_Indexes" type="xs:boolean" use="optional"/>
                <xs:attribute name="iView_Type" type="xs:int" use="optional"/>
                <xs:attribute name="sIDs_Unit" type="xs:int" use="optional"/>
                <xs:attribute name="sUnit" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XLT">
        <xs:annotation>
            <xs:documentation xml:lang="en">Link Type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XID">
                <xs:attribute name="ID_OT_Srce" type="xs:boolean" use="required"/>
                <xs:attribute name="ID_OT_Dest" type="xs:boolean" use="required"/>
                <xs:attribute name="ID_Obj_Filtering" type="xs:boolean" use="optional"/>
                <xs:attribute name="ID_Obj_Filtering_Inv" type="xs:boolean" use="optional"/>
                <xs:attribute name="bN" type="xs:boolean" use="optional"/>
                <xs:attribute name="bN_Inv" type="xs:boolean" use="optional"/>
                <xs:attribute name="bAsso" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XFT">
        <xs:annotation>
            <xs:documentation xml:lang="en">File Type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XNamed">
                <xs:attribute name="sBase_Name" type="xs:string" use="optional"/>
                <xs:attribute name="bIndex_Modifiable" type="xs:boolean" use="optional"/>
                <xs:attribute name="bBase_Name_Modifiable" type="xs:boolean" use="optional"/>
                <xs:attribute name="bVersion" type="xs:boolean" use="optional"/>
                <xs:attribute name="bDate_File" type="xs:boolean" use="optional"/>
                <xs:attribute name="sRDir" type="xs:string" use="required"/>
                <xs:attribute name="bRO" type="xs:boolean" use="optional"/>
                <xs:attribute name="sTitle_Index" type="xs:string" use="optional"/>
                <xs:attribute name="bAlpha_Index" type="xs:boolean" use="optional"/>
                <xs:attribute name="sTitle_Version" type="xs:string" use="optional"/>
                <xs:attribute name="bAlpha_Version" type="xs:boolean" use="optional"/>
                <xs:attribute name="iFIT" type="xs:int" use="optional"/>
                <xs:attribute name="iTag" type="xs:int" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XConv">
        <xs:annotation>
            <xs:documentation xml:lang="en">Conversion between two unit (Unit2= fA x Unit1+fB).</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XID">
                <xs:attribute name="fA" type="xs:float" use="required"/>
                <xs:attribute name="fB" type="xs:float" use="required"/>
                <xs:attribute name="ID_Unit1" type="xs:int" use="required"/>
                <xs:attribute name="ID_Unit2" type="xs:int" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XST">
        <xs:annotation>
            <xs:documentation xml:lang="en">Series Type (for table data). Owned by a table type.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XNamed">
                <xs:attribute name="ID_Unit" type="xs:int" use="optional"/>
                <xs:attribute name="bN" type="xs:boolean" use="optional"/>
                <xs:attribute name="bNum" type="xs:boolean" use="optional"/>
                <xs:attribute name="ID_TT" use="optional    "/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XTT">
        <xs:annotation>
            <xs:documentation xml:lang="en">Table Type (for table data).</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XID">
                <xs:sequence>
                    <xs:element name="ST" type="XST" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="iGrapher" type="xs:int" use="optional"/>
                <xs:attribute name="sName" type="xs:string" use="optional"/>
                <xs:attribute name="sPath_File" type="xs:string" use="optional"/>
                <xs:attribute name="sRPath_File" type="xs:string" use="optional"/>
                <xs:attribute name="bMOH" type="xs:boolean" use="optional"/>
                <xs:attribute name="bSOH" type="xs:boolean" use="optional"/>
                <xs:attribute name="bMCH" type="xs:boolean" use="optional"/>
                <xs:attribute name="iTag" type="xs:integer" use="optional"/>
                <xs:attribute name="sDescription" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XSrc">
        <xs:annotation>
            <xs:documentation xml:lang="en">Source object.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XID">
                <xs:sequence>
                    <xs:element name="Obj" type="XObj" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
                <xs:attribute name="ID_Att" type="xs:int" use="required"/>
                <xs:attribute name="ID_Obj" type="xs:int" use="required"/>
                <xs:attribute name="ID_Obj_Src" type="xs:int" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XASL">
        <xs:annotation>
            <xs:documentation xml:lang="en">Attributes Set Level</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XID">
                <xs:attribute name="ID_AS" type="xs:int" use="optional"/>
                <xs:attribute name="ID_Att" type="xs:int" use="required"/>
                <xs:attribute name="iType" type="xs:int" use="optional"/>
                <xs:attribute name="sFormat_Date" type="xs:string" use="optional"/>
                <xs:attribute name="iCol" type="xs:int" use="optional"/>
                <xs:attribute name="sXML" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XAS">
        <xs:annotation>
            <xs:documentation xml:lang="en">Attributes Set</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="XNamed">
                <xs:sequence>
                    <xs:element name="ASL" type="XASL" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="ID_OT" type="xs:int" use="optional"/>
                <xs:attribute name="sDescription" type="xs:int" use="optional"/>
                <xs:attribute name="iType" type="xs:int" use="optional"/>
                <xs:attribute name="ID_Applied_IO" type="xs:int" use="optional"/>
                <xs:attribute name="ID_Exportation" type="xs:int" use="optional"/>
                <xs:attribute name="ID_Data_Table" type="xs:int" use="optional"/>
                <xs:attribute name="bTrigger_MA" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="XExport">
        <xs:annotation>
            <xs:documentation xml:lang="en">Exportation settings</xs:documentation>
        </xs:annotation>
